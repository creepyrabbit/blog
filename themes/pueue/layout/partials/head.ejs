<% const pageTitle = page.title ? `${page.title} - ${config.title}` :
config.title; const pageDescription = page.content ? truncate(page.content, {
length: 200 }) : config.description; const pageUrl = page.title ? page.permalink
: config.url; %>

<head>
  <meta charset="UTF-8" />
  <title><%=pageTitle%></title>
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/github-fork-ribbon-css/0.2.3/gh-fork-ribbon.min.css"
  />

  <%- open_graph({ title: pageTitle, type: page.title ? "article" : "blog", url:
  pageUrl, image: `${config.url}/logo.jpg`, site_name: config.title,
  description: pageDescription, }) %> <%- autoCanonical(config, page) %>

  <link
    href="https://fonts.googleapis.com/css?family=Noto+Sans+KR:400,700,900"
    rel="stylesheet"
  />
  <% if (theme.stylesheets !== undefined && theme.stylesheets.length > 0) { %>
  <!-- stylesheets list from _config.yml -->
  <% theme.stylesheets.forEach(url => { %> <%- css(url) %> <% }); %> <% } %>

  <!-- googlt analytics -->
  <script>
    (function (i, s, o, g, r, a, m) {
      i["GoogleAnalyticsObject"] = r;
      (i[r] =
        i[r] ||
        function () {
          (i[r].q = i[r].q || []).push(arguments);
        }),
        (i[r].l = 1 * new Date());
      (a = s.createElement(o)), (m = s.getElementsByTagName(o)[0]);
      a.async = 1;
      a.src = g;
      m.parentNode.insertBefore(a, m);
    })(
      window,
      document,
      "script",
      "//www.google-analytics.com/analytics.js",
      "ga"
    );

    ga("create", "UA-57477390-1", "auto");
    ga("send", "pageview");
  </script>
</head>
